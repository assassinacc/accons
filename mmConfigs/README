/* INDEX */

[dromoman.cfg]
 - ADD DROMOMAN TO R&D TECHTREE

[infernalrobotics.cfg]
 - ADD INFERNAL ROBOTICS TO R&D TECHTREE

[mechjeb.cfg]
 - ADD MJ TO COMMAND PODS AND PROBE CORES
 - REMOVE MJ SUBMODULES R&D TECHTREE RESTRICTION

[remotetech.cfg]
 - ADD REMOTETECH ANTENNA TO COMMAND PODS



